public class ForLoop {
    public static void loop(){

        for (int i = 0; i < 10; i += 1) {
            System.out.println("Hello " + i);
        }

        for (int i = 10; i > 0; i -= 1) {
            System.out.println("2 ^ " + i + " = " + (int) Math.pow(2, i));
        }

        //printing A to Z
        for (char i = 65; i < 91; i += 1) {
            System.out.print(i + " ");
        }
    }
}
